<?xml version="1.0" encoding="utf-8"?>
<Request>
	<ClientNO>${kpddm!}</ClientNO>
	<SerialNumber>${iurb.jylsh!}</SerialNumber>
	<InvType>12</InvType>
	<Spbmbbh>12.0</Spbmbbh>
	<Drawer>${iurb.kpr!}</Drawer>
	<Payee>${iurb.skr!}</Payee>
	<Reviewer>${iurb.fhr!}</Reviewer>
	<Seller>
		<Identifier>${iurb.xfsh!}</Identifier>
		<Name>${iurb.xfmc!}</Name>
		<Address>${iurb.xfdz!}</Address>
		<TelephoneNo>${iurb.xfdh!}</TelephoneNo>
		<Bank>${iurb.xfyh!}</Bank>
		<BankAcc>${iurb.xfyhzh!}</BankAcc>
	</Seller>
	<OrderSize count="1">
		<Order>
			<OrderMain>
				<OrderNo>${iurb.ddh!}</OrderNo>
				<!-- OrderNo必须, 每笔订单号必须唯一，来源系统订单号，String20-->
				<InvoiceList>0</InvoiceList>
				<!--纸质票必须,是否打印清单 1 打印清单 0 不打印清单-->
				<InvoiceSplit>1</InvoiceSplit>
				<!--InvoiceSplit必须，超过最大开票限额或单张发票可开具行，是否自动拆分？（1、拆分；0、不拆分）-->
				<InvoiceSfdy>1</InvoiceSfdy>
				<!-- InvoiceSfdy必须，是否立即打印，1写税控设备并打印，0写税控设备不打印-->
				<OrderDate>${jylssj!}</OrderDate>
				<!--OrderDate可选，来源系统订单时间，"YYYY-MM-DD HH24:MI:SS"格式-->
				<ChargeTaxWay>0</ChargeTaxWay>
				<!--ChargeTaxWay可选，征税方式，0-普通征税，1-减按征税，2-差额征税，String1-->
				<TotalAmount>${iurb.jshj?c!}</TotalAmount>
				<!--TotalAmount必须，价税合计，小数点后2位小数，该栏目打印在发票上-->
				<TaxMark>${iurb.hsbz!}</TaxMark>
				<!--TaxMark必须，交易流水中的金额是否含税？（1、含税；0、不含税）-->
				<Remark>${iurb.bz!}</Remark>
				<!--Remark可选，该栏目打印在发票上的备注，String200，该栏目打印在发票上-->
				<ExtractedCode>${iurb.ddh!}</ExtractedCode>
				<!--ExtractedCode，可选，提取码，第三方系统产生，必须唯一，客户可以在网站上输入后根据提取码匹配交易信息和下载电子发票String100-->
				<Buyer>
					<CustomerType>0</CustomerType>
					<!--CustomerType必选，客户类型（0个人、1公司），专票和需要报销的普票，客户类型为1-->
					<Identifier>${iurb.gfsh!}</Identifier>
					<!--Identifier可选，购买方税号String20，15、18或20位，该栏目打印在发票上，客户类型为1时必须-->
					<Name>${iurb.gfmc!}</Name>
					<!--Name必须，购买方名称String100，该栏目打印在发票上-->
					<Address>${iurb.gfdz!}</Address>
					<!--Address可选，购买方地址String100，该栏目打印在发票上，专用发票必须-->
					<TelephoneNo>${iurb.gfdh!}</TelephoneNo>
					<!--TelephoneNo可选，购买方电话String20，该栏目打印在发票上，专用发票必须-->
					<Bank>${iurb.gfyh!}</Bank>
					<!--Bank可选，购买方银行String100，该栏目打印在发票上，专用发票必须-->
					<BankAcc>${iurb.gfyhzh!}</BankAcc>
					<!--BankAcc可选，购买方银行账号String30，该栏目打印在发票上，专用发票必须-->
					<Email>${iurb.gfemail!}</Email>
					<!--Email可选，客户接收电子发票的电子邮箱地址String50-->
					<IsSend>${iurb.sffsyj!}</IsSend>
					<!--IsSend电子发票生成后是否发送电子邮件（1、是；0、否）-->
					<Recipient>${iurb.gfsjr!}</Recipient>
					<!--Recipient可选，纸质发票收件人姓名String50-->
					<ReciAddress></ReciAddress>
					<!--Address可选，纸质发票收件人地址String200-->
					<Zip>${iurb.gfyb!}</Zip>
					<!--zip可选，纸质发票收件人邮编String10-->
				</Buyer>
			</OrderMain>
			<OrderDetails count="${count}">
				<!--size收费明细商品或服务行数，必须与ProductItem数量一致-->
				 <#list mxList as iurdb>
				<ProductItem>
					<VenderOwnCode></VenderOwnCode>
					<!--可空，商品或收费项目的自行编码-->
					<ProductCode>${iurdb.spdm!}</ProductCode>
					<!--ProductCode必选，对应Spbmbbh的商品和服务税收分类编码String19 -->
					<ProductName>${iurdb.spmc!}</ProductName>
					<!--ProductName必须，商品名称String30，该栏目打印在发票上-->
					<RowType>0</RowType>
					<!--RowType必须，发票行性质（0、正常行；1、折扣行；2、被折扣行）。比如充电器单价100元，折扣10元，则明细为2行，充电器行性质为2，折扣行性质为1。如果充电器没有折扣，则值应为0-->
					<Spec></Spec>
					<!--Spec可选，商品规格型号String20，该栏目打印在发票上-->
					<Unit>${iurdb.spdw!}</Unit>
					<!--Unit可选，商品单位String20，该栏目打印在发票上-->
					<!--<Quantity></Quantity>-->
					<!--Quantity可选，商品数量，该栏目打印在发票上-->
					<!--<UnitPrice></UnitPrice>-->
					<!--UnitPrice可选，商品单价，如果TaxMark='1'，此单价为含税单价，否则不含税单价，该栏目打印在发票上-->
					<Amount>${iurdb.spje?c!}</Amount>
					<!--Amount必须，商品金额，如果TaxMark='1'，此金额为含税金额，否则不含税金额，该栏目打印在发票上-->
					<DeductAmount></DeductAmount>
					<!--可空，ChargeTaxWay=2差额征收时必须，小数点后保留2位-->
					<TaxRate>${iurdb.spsl!}</TaxRate>
					<!--TaxRate必须，商品税率，税率只能为0或0.03或0.04或0.06或0.11或0.13或0.17，该栏目打印在发票上-->
					<TaxAmount>${iurdb.spse?c!}</TaxAmount>
					<!--TaxAmount，商品税额，如果TaxMark='0'，商品税额必须，TaxMark='1'不须接入平台后由平台进行价税分离计算税额，该栏目打印在发票上-->
					<MxTotalAmount>${iurdb.jshj?c!}</MxTotalAmount>
					<!--MxTotalAmount，商品明细价税合计，必须-->
					<PolicyMark></PolicyMark>
					<!--必须，0不使用优惠政策，1使用优惠政策。如果是“1”，优惠政策名称（PolicyName）必须与《商品和税收服务分类编码表.xlsx》表格中的该编码的优惠政策名称一一对应。-->
					<TaxRateMark></TaxRateMark>
					<!--必须,空或0是正常税率,1是免税。如果是免税，则税率（TaxRate）和税额（TaxAmount）必须为“0”，优惠政策标识必填“1”，且优惠政策（PolicyName）必须填“免税”。2是不征税,3普通零税率-->
					<PolicyName></PolicyName>
					<!--优惠政策标识PolicyMark=1时必须，对应《商品和税收服务分类编码表.xlsx》表格中的“优惠政策名称”列的内容。如果优惠政策标识是“1”，此字段必填。-->
				</ProductItem>
				</#list>
			</OrderDetails>
		</Order>
	</OrderSize>
</Request>
